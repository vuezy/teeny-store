    "coverage": "vitest run --coverage",
    "build": "vite build",
    "pack": "npm pack --pack-destination dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/coverage-v8": "^3.2.2",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1",
